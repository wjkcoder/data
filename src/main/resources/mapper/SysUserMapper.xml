<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.charsmart.data.mapper.SysUserMapper">
    <select id="queryUser" resultType="com.charsmart.data.entity.SysUser">
        select * from sys_user t where t.user_name = #{name}
    </select>
</mapper>
